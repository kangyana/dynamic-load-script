{
  "name": "dynamic-load-script",
  "version": "1.0.1",
  "description": "Dynamic loading (CDN / local script)",
  "main": "./dist/load-script.min.js",
  "module": "./dist/load-script.esm.js",
  "license": "MIT",
  "scripts": {
    "bootstrap": "yarn",
    "build": "rollup -c ./build/rollup.config.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "devDependencies": {
    "@umijs/fabric": "^2.10.1",
    "rollup": "^2.67.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}